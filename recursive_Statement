    class Program {
    static void OddNumbers(int a)
    {
          if (a >= 1)
            {
              Console.Write("{0}\n", a);
              OddNumbers(a - 2);
          }
    }
    public static int Main(string[] args)
    {
          int Number = 9;
          Console.WriteLine("Odd Numbers");
          Number = (Number % 2 == 0) ? Number-1 : Number;
          OddNumbers(Number);
          return 0;
    }
    }
